{ @openapi({
    openapi: "3.0.0",
    info: {
      title: "Sample Api",
      version: "0.1.0",
    },
  })
  createUser: { @endpoint({summary: "create a user"})
    route: "POST /users",
    req: {
      body: {
        firstName: "foo",
        lastName: "bar",
      }
    },
    res: {
      200: {
        firstName: "foo",
        lastName: "bar",
        role: "user",
      }
    }
  }
}
